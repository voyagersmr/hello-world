         <script type="text/javascript">
         var mySwiper = new Swiper ('.swiper-container',{
           /* direction:'vertical',*/
            loop: true,
            autoplay:true,



            //如果需要分页器
            pagination:{
                el: '.swiper-pagination',
            },
            //如果需要钱就后退按钮
            navigation:{
                nextEl:'.swiper-button-next',
                prevEl:'.swiper-button-prev',
            },

            //如果需要滚动条
            scrollbar:{
                el:'.swiper-scrollbar',
            },
         })

         </script>



<script>
     $('.content .tab li').click(function(){
        $(this).addClass('pot');
        $(this).siblings().removeClass('pot');
        var cur = $(this).index();
        $('.content .tab-detail').eq(cur).show();
        $('.content .tab-detail').eq(cur).siblings().hide();
        $('.content .tab').eq(0).show();


     })
</script>

     <script>
     $('.footer li').click(function(){
        var _index = $(this).index();
       /* alert(_index)*/
        $(this).addClass('active');
        $(this).siblings().removeClass('active')
        $(this).css({background:"url(images/bg0"+_index+".png) no-repeat 50% 0px"});
        if(_index==0){
            $(this).siblings().eq(0).css({background:"url(images/bg001.png) no-repeat 50% 0px"});
            $(this).siblings().eq(1).css({background:"url(images/bg002.png) no-repeat 50% 0px"});
            $(this).siblings().eq(2).css({background:"url(images/bg003.png) no-repeat 50% 0px"});
        }
        if(_index==1){
            $(this).siblings().eq(0).css({background:"url(images/bg000.png) no-repeat 50% 0px"});
           $(this).siblings().eq(1).css({background:"url(images/bg002.png) no-repeat 50% 0px"});
           $(this).siblings().eq(2).css({background:"url(images/bg003.png) no-repeat 50% 0px"});
        }
        if(_index==2){
        
        $(this).siblings().eq(0).css({background:"url(images/bg000.png) no-repeat 50% 0px"});
        $(this).siblings().eq(1).css({background:"url(images/bg001.png) no-repeat 50% 0px"});
        $(this).siblings().eq(2).css({background:"url(images/bg003.png) no-repeat 50% 0px"});
       }
       if(_index==3){
        
        $(this).siblings().eq(0).css({background:"url(images/bg000.png) no-repeat 50% 0px"});
        $(this).siblings().eq(1).css({background:"url(images/bg001.png) no-repeat 50% 0px"});
        $(this).siblings().eq(2).css({background:"url(images/bg002.png) no-repeat 50% 0px"});
       }

     })
</script>

